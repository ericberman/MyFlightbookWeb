    </compilers>
  </system.codedom>
  <location path="Public/AllMakes.aspx">
	<system.webServer>
		<httpRedirect enabled="true" destination="/logbook/mvc/AllMakes$S" httpResponseStatus="Found" exactDestination="true" />
